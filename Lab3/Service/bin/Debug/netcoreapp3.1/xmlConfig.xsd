<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="PathsOptions">
		<xs:complexType>
			<xs:sequence>
				<xs:element type="xs:string" name="SourceDirectory"/>
				<xs:element type="xs:string" name="TargetDirectory"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EncryptingOptions">
		<xs:complexType>
			<xs:sequence>
				<xs:element type="xs:string" name="Key"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompressOptions">
		<xs:complexType>
			<xs:sequence>
				<xs:element type="xs:string" name="CompressFormat"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>